# Coverage configuration for pytest-cov

[run]
source = src
omit =
    */tests/*
    */__pycache__/*
    */venv/*
    */.venv/*
    */site-packages/*

[report]
precision = 2
skip_empty = True
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = htmlcov
